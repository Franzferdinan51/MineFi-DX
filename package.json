{
  "name": "kdx",
  "description": "Kaspa Desktop eXperience",
  "version": "0.0.1",
  "private": true,
  "main": "kdx.js",
  "gulp-config": {
    "nwjs-version": "0.32.0",
    "node-version": "10.0.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "*",
    "colors": "^1.4.0",
    "fs-extra": "*",
    "is-docker": "*",
    "lit-element": "*",
    "mkdirp": "*",
    "pg": "*",
    "underscore": "*",
    "xterm": "*",
    "xterm-addon-fit": "*",
    "xterm-addon-web-links": "*"
  },
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "scripts": {
    "start": "nw ."
  },
  "window": {
    "frame": true,
    "transparent": false,
    "show": true
  },
  "node-remote": [
    "http://localhost"
  ],
  "chromium-args": "--disable-gpu-compositing --force-cpu-draw --enable-logging --disable-pinch --enable-experimental-web-platform-features"
}
